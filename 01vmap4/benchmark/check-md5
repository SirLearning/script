n=${#dicts[@]}
for (( i = 0; i+3 < n; i +=3 )); do
    md5sum "${dicts[i]}" >> md5sum.txt &
    md5sum "${dicts[i+1]}" >> md5sum.txt &
    md5sum "${dicts[i+2]}" >> md5sum.txt &
    md5sum "${dicts[i+3]}" >> md5sum.txt &
    wait
done

