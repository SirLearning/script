   "source": [
    "# Examples\n",
    "\n",
    "1. count $\\sum\\cos(x)dx$\n",
    "```{r}\n",
    "result <- vector(\"numeric\")\n",
    "while (length(result) < 10000) {\n",
    "  y <- runif(1, 0, pi/2)\n",
    "  u <- runif(1)\n",
    "  if (u <= cos(y)) {\n",
    "    result <- append(result, y)\n",
    "  }\n",
    "}\n",
    "judge1 <- result <= 1\n",
    "judge2 <- result >= 0\n",
    "m <- sum(judge1*judge2)"
   ]
  }
